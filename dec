pipeline
{
  agent any
  stages
  {
      stage('check validate'){
      steps{
                echo ' i am validating'
        }
      }
      stage('compile'){
      steps{
                echo ' i am compiling'
        }
      }
      stage('installation'){
      steps{
                echo ' i am installing'
        }
      }
      stage('deploy'){
      steps{
                echo ' i am deploying'
        }
      }
   }
}
